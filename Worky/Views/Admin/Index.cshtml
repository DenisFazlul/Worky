@{
}
@model List<DetailUserModel>

    <div style="display:grid">

    
 @foreach(DetailUserModel m in Model)
 {
     <div>
     <a>@m.user.Email</a>
     <a>@m.user.IsConfirmed</a>
     <a>@m.user.ConfirmCode</a>
     <a>@m.user.IsBlock</a>
     <a href="/Admin/RemoveUser?id=@m.user.Id">Удалть</a>
     <a href="/Admin/BlockUser?id=@m.user.Id">Заблокировать</a>
     <a href="/Admin/UnBlockUser?id=@m.user.Id">Разблокировать</a>

     </div>
 }
 </div>